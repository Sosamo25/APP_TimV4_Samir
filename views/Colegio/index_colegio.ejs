      <div class="Usuarios_dashboard">

      <div class="Usuarios_icon_init"></div>  
      <div class="Usuarios_title_init">Colegios</div>
      <input class="Buscador_position" type="search" placeholder="Buscar" id="Buscador" />
            <a href="/create_colegio" type="button" class="Usuarios_agregar"class="Usuarios_agregar_font">Agregar</a>  

            <div class="form_list">

            <table class="table-striped table-bordered table-striped text-left Tabla_position">
                <thead class="thead th">
                    <tr class="bg-primary">
                        <th scope="col">ID</th>
                        <th scope="col">NOMBRE</th>
                        <th scope="col">CIUDAD</th>
                        <th scope="col">NOMBRE REPRESENTANTE</th>
                        <th scope="col">CLASIFICACION</th>
                        <th scope="col">CODIGO DANE</th>
                        <th scope="col">TELEFONO</th>
                        <th scope="col">CORREO</th>
                        <th scope="col">Acciones</th>  
                    </tr>
                </thead>
                <tbody>
                    <% results.forEach((nombre)=>{ %>
                    <tr >
                        <td><%= nombre.id %></td>
                        <td><%= nombre.nombre %></td>
                        <td><%= nombre.ciudad %></td>
                        <td><%= nombre.nombre_repre %></td>
                        <td><%= nombre.clasificacion %></td>
                        <td><%= nombre.c_dane %></td>
                        <td><%= nombre.telefono %></td>
                        <td><%= nombre.correo %></td>
                        <td>
                            <div class="d-grid gap-2 d-md-block">

                                <a href="/edit_colegio/<%= nombre.id %>" class="btn btn-outline-info"><i class='bx bxs-edit'></i></a>
                                <a href="/delete_colegio/<%= nombre.id %> " class="btn btn-outline-danger"><i class='bx bxs-trash-alt' method="delete"></i></a>

                            </div>
                           
                        </td>
                    </tr>
                    <% }) %>
                </tbody>

            </table>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>    
